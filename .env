# API Authentication Key (used to secure your honeypot endpoint)
API_KEY=use-yours

# LLM Provider: "gemini" or "openai"
LLM_PROVIDER=your-choice

# Google Gemini API Key (if using Gemini)
GEMINI_API_KEY=use-yours

# OpenAI API Key (if using OpenAI)
OPENAI_API_KEY=use-yours

# Engagement threshold (number of messages before sending callback)
ENGAGEMENT_THRESHOLD=10
